<div class="row">
  <div class="col-lg-12 actions">{{we-action-create-btn model req event.id}}</div>
</div>

<table class="table table-striped table-hover">
  <thead>
    <th>{{t 'id'}}</th>
    <th>{{t 'form-cfpartner-name'}}</th>
    <th>{{t 'form-cfpartner-logo'}}</th>
    <th>{{t 'actions'}}</th>
  </thead>
  <tbody>
    {{#each data}}
      <tr>
        <td>{{id}}</td>
        <td>{{name}}</td>
        <td>
          {{#each logo}}
            {{{we-image this 'thumbnail' class="thumbnail"}}}
          {{/each}}
        </td>
        <td>
          {{we-action-view-btn 'cfpartner' this ../req eventId id }}
          {{we-action-edit-btn 'cfpartner' this ../req eventId id }}
          {{we-action-delete-btn 'cfpartner' this ../req eventId id }}
        </td>
      </tr>
    {{else}}
      <tr><td colspan="5" class="text-center">{{t 'event.cfpartner.empty'}}</td></tr>
    {{/each}}
  </tbody>
</table>